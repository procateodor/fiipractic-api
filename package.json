{
  "name": "fiipractic-api",
  "version": "1.0.0",
  "description": "FIIPractic API",
  "main": "index.js",
  "engines": {
    "node": ">=13.11.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "npm run prod",
    "dev": "NODE_ENV=dev nodemon index.js",
    "prod": "NODE_ENV=prod node index.js",
    "test": "jest",
    "coverage": "jest --coverage",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "depcheck": "depcheck ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/procateodor/fiipractic-api.git"
  },
  "keywords": [],
  "author": "Teodor Proca",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/procateodor/fiipractic-api/issues"
  },
  "homepage": "https://github.com/procateodor/fiipractic-api#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7"
  },
  "devDependencies": {
    "depcheck": "^0.9.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^25.2.7",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4"
  },
  "nodemonConfig": {
    "ignore": [
      "compare/*"
    ],
    "delay": "2500"
  },
  "jest": {
    "testRegex": ".spec.js",
    "testPathIgnorePatterns": [
      "node_modules"
    ],
    "testEnvironment": "node"
  },
  "standard": {
    "ignore": [
      "tests/**/*.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint && npm run depcheck"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "prettier --write"
    ]
  }
}
